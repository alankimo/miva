webfont = 'Nunito', sans-serif;

primario = #fdb441
base = #f4f4f4
base-dark = #999999

border-radius()
	-webkit-border-radius arguments
	-moz-border-radius arguments
	border-radius arguments

transition()
	-webkit-transition:all arguments ease-in;  
	-moz-transition:all arguments ease-in;  
	-o-transition:all arguments ease-in;  
	-ms-transition:all arguments ease-in;  
	transition:all arguments ease-in; 
	
box-sizing()
	-webkit-box-sizing: arguments;
	-moz-box-sizing: arguments;
	box-sizing: arguments; 
	
text-shadow()
	text-shadow: arguments;